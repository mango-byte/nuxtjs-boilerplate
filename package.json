{
  "name": "nuxtjs-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "start:ci": "nuxt start --dotenv .env.ci",
    "generate:ci": "nuxt generate --dotenv .env.ci",
    "start:stg": "nuxt start --dotenv .env.stg",
    "build:stg": "nuxt build --dotenv .env.stg",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{css,scss,sass,html,vue}\" --ignore-path .gitignore",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix && yarn lint:style --fix",
    "prepare": "husky install",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:compose": "docker-compose up --abort-on-container-exit --build"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache",
    "*.{css,scss,sass,html,vue}": "stylelint",
    "*.**": "prettier --check --ignore-unknown"
  },
  "dependencies": {
    "@chantouchsek/validatorjs": "^1.0.1",
    "@nuxtjs/auth-next": "5.0.0-1641544397.1af946a",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/component-cache": "^1.1.6",
    "@nuxtjs/dayjs": "^1.4.1",
    "@nuxtjs/i18n": "^7.2.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/universal-storage": "^0.5.9",
    "collect.js": "^4.30.7",
    "core-js": "^3.20.2",
    "lodash": "^4.17.21",
    "nuxt": "^2.15.8",
    "nuxt-axios-cancel": "^0.0.2",
    "nuxt-property-decorator": "^2.9.1",
    "nuxt-vuex-router-sync": "^0.0.3",
    "vue-axios-http": "^1.2.3",
    "vue-waiting": "^0.0.4",
    "vuetify": "^2.6.2",
    "vuetify-dialogue": "^0.0.7"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.3",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/google-fonts": "^1.3.0",
    "@nuxtjs/stylelint-module": "^4.1.0",
    "@nuxtjs/vuetify": "^1.12.3",
    "@playwright/test": "^1.17.1",
    "@types/lodash": "^4.14.178",
    "@vue/test-utils": "^1.3.0",
    "@vue/vue2-jest": "^27.0.0-alpha.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.4.4",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "jest": "^27.4.4",
    "lint-staged": "^12.1.7",
    "nuxt-webpack-optimisations": "^2.1.2",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.1.0",
    "stylelint-config-standard": "^24.0.0",
    "ts-jest": "^27.1.1",
    "vue-jest": "^3.0.4"
  }
}
